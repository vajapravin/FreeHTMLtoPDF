{
  "manifest_version": 2,

  "name": "FreeHTMLtoPDF",
  "description": "Free Convert HTML to PDF based on http://freehtmltopdf.com/ APIs. Enter URL to convert web page to PDF.",
  "version": "1.0",
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
